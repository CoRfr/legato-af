sandboxed: false

executables:
{
    secStore = ( $LEGATO_ROOT/components/secStore/secStoreDaemon )
}

processes:
{
    run:
    {
        (secStore)
    }

    faultAction: restart
}

provides:
{
    api:
    {
        secStore.secStoreDaemon.le_secStore
    }
}


bindings:
{
    secStore.secStoreDaemon.le_appInfo -> <root>.le_appInfo
    secStore.secStoreDaemon.le_instStat -> <root>.le_instStat
}

